    "cors": "^2.8.5",
    "express": "^4.16.4",
    "express-graphql": "^0.9.0",
    "express-session": "^1.15.6",
    "graphql": "^14.4.2",
    "graphql-tools": "^4.0.5",
    "lodash": "^4.17.15",
    "merge-graphql-schemas": "^1.7.0",
    "mongodb": "^3.1.13",
    "mongoose": "^5.4.19",
    "nodemailer": "^6.1.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^2.0.0",
    "uuid": "^3.3.2",
    "vue": "^2.6.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
