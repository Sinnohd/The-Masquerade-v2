{
  "name": "the-masquerade",
  "version": "1.0.0",
  "description": "Web app to manage The Masquerade Chronicle",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --exec babel-node ./src/app.js",
    "build": "babel ./src --out-dir ./dist && cp src/client_secret.json dist/client_secret.json && cp src/graphql/api.graphql dist/graphql/api.graphql",
    "serve": "node ./dist/app.js"
  },
